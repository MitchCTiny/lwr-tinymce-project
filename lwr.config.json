{
    "lwc": { "modules": [{ "dir": "$rootDir/src/modules" }] },
    "routes": [
        {
            "id": "example",
            "path": "/",
            "rootComponent": "example/app",
            "bootstrap": {
                "syntheticShadow": true
            }
        }
    ],
    "assets": [
        {
            "alias": "tinymce",
            "dir": "$rootDir/node_modules/tinymce",
            "urlPath": "/tinymce"
        },
        {
            "alias": "assets",
            "dir": "$rootDir/src/assets",
            "urlPath": "/assets"
        }
    ],
    "locker": {
        "enabled": false
    }
}
